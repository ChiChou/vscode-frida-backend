{
  "name": "vscode-frida-backend",
  "version": "0.0.1",
  "description": "backend for vscode-frida",
  "engines": {
    "node": ">= 12.13 <13"
  },
  "bin": {
    "vscode-frida-server": "build/src/cli.js"
  },
  "main": "build/src/cli.js",
  "repository": "https://github.com/ChiChou/vscode-frida-backend.git",
  "devDependencies": {
    "@types/frida-gum": "^15.0.0",
    "@types/jest": "~24.0.25",
    "@types/node": "~12.12.22",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "~2.14.0",
    "@typescript-eslint/parser": "~2.14.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.9.0",
    "eslint-plugin-jest": "~23.2.0",
    "frida-compile": "^9.1.0",
    "jest": "~24.9.0",
    "prettier": "~1.19.1",
    "rimraf": "~3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "~24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tsutils": "~3.17.0",
    "typescript": "~3.7.4"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "agent:build": "cd agent && frida-compile src/index.ts -o build/agent.js",
    "agent:watch": "npm run agent:build -- -w"
  },
  "author": "CodeColorist <codecolorist@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.1.1",
    "fast-png": "^5.0.1",
    "frida": "^12.8.6",
    "koa": "^2.11.0",
    "koa-router": "^7.4.0",
    "tslib": "~1.10.0"
  }
}
